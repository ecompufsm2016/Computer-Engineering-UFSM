#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1, n2, adicao;
    printf("Digite n1: ");
    scanf("%d", &n1);
    printf("Digite n2: ");
    scanf("%d", &n2);
    adicao = (n1+n2);
    if (adicao>20) {
            adicao = adicao + 8;
            printf("Valor = %d\n", adicao);
    }
    else {
        adicao = adicao -5;
        printf("Valor = %d\n", adicao);
}
     if (adicao <=20)
return 0;
    }
